        },
        "foreigns" : {
            "CREDENTIAL_ID" : { "schema" : "Credential" }
        },
        "hasTimestamps" : false,
        "canCreate"     : false,
        "canEdit"       : true,
        "canDelete"     : false,
        "canRemove"     : true
