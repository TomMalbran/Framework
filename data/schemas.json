{
    "credentials" : {
        "table"  : "credentials",
        "fields" : {
            "CREDENTIAL_ID"    : { "type" : "id",      "name" : "credentialID"                   },
            "currentUser"      : { "type" : "number",                          "cantEdit" : true },
            "email"            : { "type" : "string"                                             },
            "firstName"        : { "type" : "string"                                             },
            "lastName"         : { "type" : "string"                                             },
            "phone"            : { "type" : "string",                          "noExists" : true },
            "language"         : { "type" : "string"                                             },
            "avatar"           : { "type" : "string",                          "cantEdit" : true },
            "access"           : { "type" : "string",                          "cantEdit" : true },
            "password"         : { "type" : "string",                          "cantEdit" : true },
            "salt"             : { "type" : "string",                          "cantEdit" : true },
            "reqPassChange"    : { "type" : "boolean",                         "cantEdit" : true },
            "passExpiration"   : { "type" : "date",                            "cantEdit" : true },
            "accessToken"      : { "type" : "string",                          "cantEdit" : true },
            "tokenExpiration"  : { "type" : "date",                            "cantEdit" : true },
            "status"           : { "type" : "string",                          "noExists" : true },
            "observations"     : { "type" : "text",                            "noExists" : true },
            "sendEmails"       : { "type" : "boolean",                         "noExists" : true },
            "sendEmailNotis"   : { "type" : "boolean",                         "noExists" : true },
            "sendTickets"      : { "type" : "boolean",                         "noExists" : true },
            "timezone"         : { "type" : "number",  "isSigned" : true,      "cantEdit" : true },
            "currentLogin"     : { "type" : "date",                            "cantEdit" : true },
            "lastLogin"        : { "type" : "date",                            "cantEdit" : true },
            "askNotifications" : { "type" : "boolean", "default"  : 1,         "cantEdit" : true }
        },
        "expressions" : {
            "fullName" : {
                "expression" : "IF(credentials.lastName <> '' AND credentials.firstName <> '', CONCAT(credentials.firstName, ' ', credentials.lastName), IF(credentials.firstName <> '', credentials.firstName, credentials.lastName))",
                "type"       : "string"
            }
        },
        "hasTimestamps" : true,
        "canCreate"     : true,
        "canEdit"       : true,
        "canDelete"     : true
    },
    "devices" : {
        "table"  : "credentials_devices",
        "fields" : {
            "CREDENTIAL_ID" : { "type" : "number", "name" : "credentialID", "isPrimary" : true },
            "userAgent"     : { "type" : "string",                          "isPrimary" : true },
            "playerID"      : { "type" : "string"                                              }
        },
        "foreigns" : {
            "CREDENTIAL_ID" : { "table" : "credentials" }
        },
        "hasTimestamps" : true,
        "canCreate"     : false,
        "canEdit"       : true,
        "canDelete"     : false
    },
    "refreshTokens" : {
        "table"  : "credentials_refresh_tokens",
        "fields" : {
            "CREDENTIAL_ID"  : { "type" : "number", "name" : "credentialID"                    },
            "refreshToken"   : { "type" : "string",                         "isPrimary" : true },
            "userAgent"      : { "type" : "string"                                             },
            "expirationTime" : { "type" : "date"                                               }
        },
        "foreigns" : {
            "CREDENTIAL_ID" : { "table" : "credentials" }
        },
        "hasTimestamps" : true,
        "canCreate"     : true,
        "canEdit"       : true,
        "canDelete"     : false
    },
    "resets" : {
        "table"  : "credentials_reset",
        "fields" : {
            "CREDENTIAL_ID" : { "type" : "number", "name" : "credentialID", "isPrimary" : true },
            "email"         : { "type" : "string",                          "isPrimary" : true },
            "code"          : { "type" : "string"                                              },
            "time"          : { "type" : "number"                                              }
        },
        "foreigns" : {
            "CREDENTIAL_ID" : { "table" : "credentials" }
        },
        "hasTimestamps" : false,
        "canCreate"     : false,
        "canEdit"       : false,
        "canDelete"     : false
    },
    "spam" : {
        "table"  : "credentials_spam",
        "fields" : {
            "ip"   : { "type" : "string", "isPrimary" : true },
            "time" : { "type" : "number"                     }
        },
        "hasTimestamps" : false,
        "canCreate"     : false,
        "canEdit"       : false,
        "canDelete"     : false
    },
    "storage" : {
        "table"  : "credentials_storage",
        "fields" : {
            "CREDENTIAL_ID" : { "type" : "number", "name" : "credentialID", "isPrimary" : true },
            "bucket"        : { "type" : "string",                          "isPrimary" : true },
            "data"          : { "type" : "string"                                              },
            "time"          : { "type" : "number"                                              }
        },
        "foreigns" : {
            "CREDENTIAL_ID" : { "table" : "credentials" }
        },
        "hasTimestamps" : false,
        "canCreate"     : false,
        "canEdit"       : false,
        "canDelete"     : false
    },



    "emailTemplates" : {
        "table"  : "email_templates",
        "fields" : {
            "TEMPLATE_ID"  : { "type" : "id",     "name"     : "templateID" },
            "templateCode" : { "type" : "string", "cantEdit" : true         },
            "language"     : { "type" : "string", "cantEdit" : true         },
            "languageName" : { "type" : "string", "cantEdit" : true         },
            "description"  : { "type" : "string", "cantEdit" : true         },
            "type"         : { "type" : "string"                            },
            "sendTo"       : { "type" : "json"                              },
            "subject"      : { "type" : "string"                            },
            "message"      : { "type" : "text"                              },
            "position"     : { "type" : "number", "cantEdit" : true         }
        },
        "hasTimestamps" : false,
        "canCreate"     : false,
        "canEdit"       : false,
        "canDelete"     : false
    },
    "emailQueue" : {
        "table"  : "email_queue",
        "fields" : {
            "EMAIL_ID"     : { "type" : "id",    "name" : "emailID" },
            "templateCode" : { "type" : "string"                    },
            "sendTo"       : { "type" : "json"                      },
            "subject"      : { "type" : "string"                    },
            "message"      : { "type" : "text"                      },
            "emailResult"  : { "type" : "string"                    },
            "sentTime"     : { "type" : "date"                      },
            "dataID"       : { "type" : "number"                    }
        },
        "expressions" : {
            "isPending" : {
                "expression" : "IF(emailResult = 'NotProcessed', 1, 0)",
                "type"       : "boolean"
            },
            "isError" : {
                "expression" : "IF(emailResult <> 'Sent', 1, 0)",
                "type"       : "boolean"
            }
        },
        "hasTimestamps" : true,
        "canCreate"     : true,
        "canEdit"       : false,
        "canDelete"     : false
    },
    "emailWhiteList" : {
        "table"  : "email_white_list",
        "fields" : {
            "EMAIL_ID"    : { "type" : "id",    "name" : "emailID" },
            "email"       : { "type" : "string"                    },
            "description" : { "type" : "string"                    }
        },
        "hasTimestamps" : true,
        "canCreate"     : true,
        "canEdit"       : true,
        "canDelete"     : false
    },
    "notificationQueue" : {
        "table"  : "notification_queue",
        "fields" : {
            "NOTIFICATION_ID" : { "type" : "id",     "name" : "notificationID" },
            "CREDENTIAL_ID"   : { "type" : "number", "name" : "credentialID"   },
            "currentUser"     : { "type" : "number"                            },
            "title"           : { "type" : "string"                            },
            "body"            : { "type" : "string"                            },
            "url"             : { "type" : "string"                            },
            "type"            : { "type" : "string"                            },
            "dataID"          : { "type" : "number"                            },
            "externalID"      : { "type" : "string"                            },
            "sentTime"        : { "type" : "number"                            },
            "isRead"          : { "type" : "boolean"                           },
            "isDiscarded"     : { "type" : "boolean"                           }
        },
        "expressions" : {
            "credentialName" : {
                "expression" : "IF(credentials.lastName <> '' AND credentials.firstName <> '', CONCAT(credentials.firstName, ' ', credentials.lastName), IF(credentials.firstName <> '', credentials.firstName, credentials.lastName))",
                "type"       : "string"
            }
        },
        "joins" : {
            "CREDENTIAL_ID" : {
                "table"     : "credentials",
                "prefix"    : "credential",
                "fieldKeys" : [ "firstName", "lastName" ]
            }
        },
        "hasTimestamps" : true,
        "canCreate"     : true,
        "canEdit"       : false,
        "canDelete"     : false
    },
    "settings" : {
        "table"  : "settings",
        "fields" : {
            "section"  : { "type" : "string", "isPrimary" : true },
            "variable" : { "type" : "string", "isPrimary" : true },
            "value"    : { "type" : "text"                       },
            "type"     : { "type" : "string"                     }
        },
        "hasTimestamps" : true,
        "canCreate"     : false,
        "canEdit"       : true,
        "canDelete"     : false
    },



    "logIDs" : {
        "table"  : "log_ids",
        "fields" : {
            "CREDENTIAL_ID" : { "type" : "number", "name" : "credentialID", "isPrimary" : true },
            "SESSION_ID"    : { "type" : "number", "name" : "sessionID"                        }
        },
        "foreigns" : {
            "CREDENTIAL_ID" : { "table" : "credentials"  },
            "SESSION_ID"    : { "table" : "log_sessions" }
        },
        "hasTimestamps" : false,
        "canCreate"     : false,
        "canEdit"       : false,
        "canDelete"     : false
    },
    "logSessions" : {
        "table"  : "log_sessions",
        "fields" : {
            "SESSION_ID"    : { "type" : "id",     "name" : "sessionID"    },
            "CREDENTIAL_ID" : { "type" : "number", "name" : "credentialID" },
            "USER_ID"       : { "type" : "number", "name" : "userID"       },
            "ip"            : { "type" : "string"                          },
            "userAgent"     : { "type" : "string"                          }
        },
        "joins" : {
            "CREDENTIAL_ID" : {
                "table"  : "credentials",
                "prefix" : "credential",
                "fields" : {
                    "firstName" : { "type" : "string", "mergeTo" : "name" },
                    "lastName"  : { "type" : "string", "mergeTo" : "name" },
                    "email"     : { "type" : "string"                     }
                }
            }
        },
        "hasTimestamps" : true,
        "canCreate"     : true,
        "canEdit"       : false,
        "canDelete"     : false
    },
    "logActions" : {
        "table"  : "log_actions",
        "fields" : {
            "ACTION_ID"     : { "type" : "id",     "name" : "actionID"     },
            "SESSION_ID"    : { "type" : "number", "name" : "sessionID"    },
            "CREDENTIAL_ID" : { "type" : "number", "name" : "credentialID" },
            "USER_ID"       : { "type" : "number", "name" : "userID"       },
            "module"        : { "type" : "string"                          },
            "action"        : { "type" : "string"                          },
            "dataID"        : { "type" : "text"                            }
        },
        "joins" : {
            "SESSION_ID" : {
                "table"  : "log_sessions",
                "prefix" : "session",
                "fields" : {
                    "createdTime" : { "type" : "date"   },
                    "ip"          : { "type" : "string" },
                    "userAgent"   : { "type" : "string" }
                }
            },
            "CREDENTIAL_ID" : {
                "table"  : "credentials",
                "prefix" : "credential",
                "fields" : {
                    "firstName" : { "type" : "string", "mergeTo" : "name" },
                    "lastName"  : { "type" : "string", "mergeTo" : "name" },
                    "email"     : { "type" : "string"                     }
                }
            }
        },
        "hasTimestamps" : true,
        "canCreate"     : true,
        "canEdit"       : false,
        "canDelete"     : false
    },
    "logDevices" : {
        "table"  : "log_devices",
        "fields" : {
            "LOG_ID"        : { "type" : "id",     "name" : "logID"        },
            "CREDENTIAL_ID" : { "type" : "number", "name" : "credentialID" },
            "userAgent"     : { "type" : "string"                          },
            "playerID"      : { "type" : "string"                          },
            "wasAdded"      : { "type" : "boolean"                         }
        },
        "hasTimestamps" : true,
        "canCreate"     : true,
        "canEdit"       : false,
        "canDelete"     : false
    },
    "logQueries" : {
        "table"  : "log_queries",
        "fields" : {
            "LOG_ID"      : { "type" : "id",   "name" : "logID" },
            "expression"  : { "type" : "text"                   },
            "environment" : { "type" : "string"                 },
            "amount"      : { "type" : "number"                 },
            "isResolved"  : { "type" : "boolean"                },
            "elapsedTime" : { "type" : "number"                 },
            "totalTime"   : { "type" : "number"                 },
            "updatedTime" : { "type" : "number"                 },
            "updatedUser" : { "type" : "number"                 }
        },
        "hasUsers"      : true,
        "hasTimestamps" : true,
        "canCreate"     : true,
        "canEdit"       : false,
        "canDelete"     : false
    },
    "logErrors" : {
        "table"  : "log_errors",
        "fields" : {
            "LOG_ID"      : { "type" : "id",    "name" : "logID" },
            "code"        : { "type" : "number"                  },
            "error"       : { "type" : "string"                  },
            "level"       : { "type" : "number"                  },
            "environment" : { "type" : "string"                  },
            "file"        : { "type" : "string"                  },
            "line"        : { "type" : "number"                  },
            "description" : { "type" : "text"                    },
            "backtrace"   : { "type" : "text"                    },
            "amount"      : { "type" : "number"                  },
            "isResolved"  : { "type" : "boolean"                 },
            "updatedTime" : { "type" : "date"                    }
        },
        "hasTimestamps" : true,
        "canCreate"     : true,
        "canEdit"       : false,
        "canDelete"     : false
    }
}
