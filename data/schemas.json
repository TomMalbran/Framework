            "lastName"         : { "type" : "string"                                             },
            "phone"            : { "type" : "string",                          "noExists" : true },
            "language"         : { "type" : "string"                                             },
            "avatar"           : { "type" : "string",                          "cantEdit" : true },
            "appearance"       : { "type" : "string",                          "cantEdit" : true },
