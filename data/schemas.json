                "type"       : "string"
            }
        },
        "joins" : {
            "CREDENTIAL_ID" : {
                "schema"    : "Credential",
                "prefix"    : "credential",
                "fieldKeys" : [ "firstName", "lastName" ]
            }
        },
        "hasFilters"    : true,
        "hasTimestamps" : true,
        "canCreate"     : true,
        "canEdit"       : true,
        "canDelete"     : false,
        "canRemove"     : true
