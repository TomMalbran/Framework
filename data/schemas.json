    "EmailTemplate" : {
        "fields" : {
            "TEMPLATE_ID"  : { "type" : "id"                        },
            "templateCode" : { "type" : "string", "cantEdit" : true },
            "language"     : { "type" : "string", "cantEdit" : true },
            "languageName" : { "type" : "string", "cantEdit" : true },
            "description"  : { "type" : "string", "cantEdit" : true },
            "subject"      : { "type" : "string"                    },
            "message"      : { "type" : "text"                      },
            "position"     : { "type" : "number", "cantEdit" : true }
        },
        "hasTimestamps" : false,
        "canCreate"     : false,
        "canEdit"       : false,
        "canDelete"     : false,
        "canRemove"     : false
    },
    "EmailQueue" : {
        "fields" : {
            "EMAIL_ID"     : { "type" : "id"     },
            "templateCode" : { "type" : "string" },
            "sendTo"       : { "type" : "json"   },
            "subject"      : { "type" : "string" },
            "message"      : { "type" : "text"   },
            "emailResult"  : { "type" : "string" },
            "sendTime"     : { "type" : "date"   },
            "sentTime"     : { "type" : "date"   },
            "dataID"       : { "type" : "number" }
        },
        "expressions" : {
            "isPending" : {
                "expression" : "IF(emailResult = 'NotProcessed', 1, 0)",
                "type"       : "boolean"
            },
            "isError" : {
                "expression" : "IF(emailResult <> 'Sent', 1, 0)",
                "type"       : "boolean"
            }
        },
        "hasFilters"    : true,
        "hasTimestamps" : true,
        "canCreate"     : true,
        "canEdit"       : true,
        "canDelete"     : false,
        "canRemove"     : true
    },
    "EmailWhiteList" : {
        "fields" : {
